{
  "authorization": {
    "name": "authorization",
    "description": "Bearer {JWT Token}",
    "required": true,
    "in": "header",
    "schema": {
      "description": "Bearer {JWT Token}",
      "example": "Bearer rzYfAAUCas_9LNMP1jsmesfrmUxw31Q8CkniJ0v2cbcCzcIr-fU0WoR0lZ315LO2qWiTNA39mPU-2uqdGfo-hg",
      "type": "string"
    }
  },
  "transaction_id": {
    "name": "transaction_id",
    "description": "The Transaction ID of the consumer",
    "required": false,
    "in": "header",
    "schema": {
      "description": "The Transaction ID of the consumer",
      "example": "86032cbe-a804-4c3b-86ce-ec3041e3effc",
      "type": "string",
      "pattern": "^[a-zA-Z0-9\\-\\_]*$"
    }
  }
}